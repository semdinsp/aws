    RowBox[{"(*", " ", 
     RowBox[{"short", " ", "from", " ", "datea", " ", "20111212"}], " ", 
     "*)"}], "\n", 
    RowBox[{
     RowBox[{"amzdate", "=", 
      RowBox[{"amazonFormattedDate", "[", "]"}]}], ";", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{
      "funky", " ", "amazon", " ", "date", " ", "no", " ", "dash", " ", 
       RowBox[{"no", " ", ":"}]}], " ", "*)"}], "\n", "\n", 
     RowBox[{"awssign", " ", "=", 
      RowBox[{"getAWSSignature", "[", 
       RowBox[{"mysecret", ",", "dateshort", ",", "region", ",", "service"}], 
       "]"}]}], ";", " ", 
